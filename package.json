{
  "name": "web9000v3",
  "version": "0.0.0",
  "description": "NVMS 9000 V3 Web",
  "author": "TVT Web Team",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev --host",
    "dev:test": "vite --mode test --host",
    "dev:prod": "vite --mode prod --host",
    "build": "vue-tsc --noEmit && vite --mode prod build",
    "build:dev": "vue-tsc --noEmit && vite --mode dev build",
    "build:test": "vue-tsc --noEmit && vite --mode test build",
    "preview": "vite preview --host",
    "lint": "eslint src",
    "format": "prettier --write src",
    "mirror:get": "npm config get registry",
    "mirror:set": "npm config set registry https://registry.npmmirror.com",
    "mirror:rm": "npm config rm registry",
    "backup": "git add . && git commit -m \"chore: backup\" && git push",
    "up": "npx npm-check-updates -u",
    "prepare": "husky install",
    "license": "node --import=./scripts/register.js ./scripts/licenseChecker.ts",
    "generate": "node --import=./scripts/register.js ./scripts/generate.ts"
  },
  "dependencies": {
    "@bassist/progress": "^0.2.2",
    "@bassist/utils": "^0.14.0",
    "axios": "^1.6.8",
    "crypto-js": "^4.1.1",
    "element-plus": "^2.7.7",
    "jalaliday": "^2.3.0",
    "js-base64": "^3.3.1",
    "js-md5": "^0.7.3",
    "jsencrypt": "^3.3.2",
    "jszip": "^3.10.1",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qrcode": "^1.5.3",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@bassist/eslint": "^0.5.0",
    "@bassist/tsconfig": "^0.1.1",
    "@bassist/uno": "^0.1.3",
    "@swc-node/register": "^1.10.9",
    "@swc/core": "^1.7.22",
    "@types/crypto-js": "^4.1.1",
    "@types/js-md5": "^0.7.0",
    "@types/node": "^20.11.5",
    "@types/qrcode": "^1.5.5",
    "@types/spritesmith": "^3.4.5",
    "@vitejs/plugin-vue": "^5.0.3",
    "esbuild-minify-templates": "^0.11.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "inquirer": "^12.0.1",
    "less": "^4.2.0",
    "license-checker-rseidelsohn": "^4.3.0",
    "lint-staged": "^15.2.0",
    "parse-literals": "^1.2.1",
    "postcss": "^8.4.39",
    "postcss-variable-compress": "^3.0.0",
    "prettier": "^3.2.4",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.1",
    "spritesmith": "^3.4.1",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.11",
    "vite-plugin-html": "^3.2.2",
    "vue-tsc": "^1.8.27"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
